<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="csCenter">
	<select id="selectAll" resultType="com.me.rentalme.cs.entity.CsVo">
		SELECT * FROM MINMIN
  	</select>
  	<select id="csDetail" resultType="com.me.rentalme.cs.entity.CsVo">
